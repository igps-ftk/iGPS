    ;FOR DEFINE CUTTING DATA SPANS
    PANEL_CUT_DEF: 0, $
    PANEL_CUT_DEF_STARTED: 0, $
    
    ;STYLE FOR PLOTTING
    PSYM:2, $
    
    ;TIME AXES TYPE
    TIME_AXES_TYPE: 'DYR', $
    TIME_AXES_VAL_MJD:PTR_NEW(), $
    TIME_AXES_VAL:PTR_NEW(), $
    TIME_AXES_VAL_DYR:PTR_NEW(), $
