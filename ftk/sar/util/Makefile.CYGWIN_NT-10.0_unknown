	rm -f esa_s1_tiff_EOFs.o

sar_sbas_tab_from_png : sar_sbas_tab_from_png.f  $(SLIB) 
	$(FTN) sar_sbas_tab_from_png.f  $(SLIB) -o sar_sbas_tab_from_png.CYGWIN_NT-10.0_unknown $(FFLAGS)
	rm -f sar_sbas_tab_from_png.o

sar_cal_sat_incidence : sar_cal_sat_incidence.f  $(SLIB) 
	$(FTN) sar_cal_sat_incidence.f  $(SLIB) -o sar_cal_sat_incidence.CYGWIN_NT-10.0_unknown $(FFLAGS)
	rm -f sar_cal_sat_incidence.o


.c.a:
	$(CC) -c $(CFLAGS) -I$(II) $<
	ar rv $@ *.o
	rm -f $*.o

.f.a:
	$(FTN) -c $(FFLAGS) $<
	ar rv $@ *.o
	rm -f $*.o


cleanall:
	rm -f *.a.CYGWIN_NT-10.0_unknown *.o *core map_distance sar_intf_tab_2_psxy esa_s1_tiff_EOFs sar_sbas_tab_from_png sar_cal_sat_incidence  map_distance.CYGWIN_NT-10.0_unknown sar_intf_tab_2_psxy.CYGWIN_NT-10.0_unknown esa_s1_tiff_EOFs.CYGWIN_NT-10.0_unknown sar_sbas_tab_from_png.CYGWIN_NT-10.0_unknown sar_cal_sat_incidence.CYGWIN_NT-10.0_unknown
