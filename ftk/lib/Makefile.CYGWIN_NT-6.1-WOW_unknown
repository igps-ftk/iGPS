$(CGPS_LIB)(moment.o): ../inc/cgps.h

.c.a:
	$(CC) -c $(CFLAGS) -I$(II) $<
	ar rv $@ *.o
	rm -f $*.o

.f.a:
	$(FTN) -c $(FFLAGS) $<
	ar rv $@ *.o
	rm -f $*.o


cleanall:
	rm -f *.a.CYGWIN_NT-6.1-WOW_unknown *.o *core NONE nblen dyr_2_yrdoyr ymd_2_dyr mean interval  NONE.CYGWIN_NT-6.1-WOW_unknown nblen.CYGWIN_NT-6.1-WOW_unknown dyr_2_yrdoyr.CYGWIN_NT-6.1-WOW_unknown ymd_2_dyr.CYGWIN_NT-6.1-WOW_unknown mean.CYGWIN_NT-6.1-WOW_unknown interval.CYGWIN_NT-6.1-WOW_unknown
