  IDB = WIDGET_INFO(EV.TOP, FIND_BY_UNAME='PANEL_OUTLIER_CKB_B')
  IDC = WIDGET_INFO(EV.TOP, FIND_BY_UNAME='PANEL_OUTLIER_CKB_C')
