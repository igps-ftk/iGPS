PROG=cron_get_usgs_quake_catalog

if [ "$GMT_pub" == "" ]; then
  echo "[]ERROR: environment variable GMT_pub not set!!"
  exit 1
fi

echo GMT_pub $GMT_pub

while [ "$1" != "" ]; do
  case $1 in
    *)
      echo "[$PROG]ERROR: invalid option ($1)!!"
      echo "[$PROG]Usage: ${PROG} "
      echo "[$PROG]Usage: e.g.,"
      echo "[$PROG]Usage: ${PROG} "
      exit 1
      ;;
  esac
  shift 2  
done

path=${GMT_pub}/quake/usgs/tibetan.plateau/
cd $path

if [ $? -ne 0 ]; then
  echo "[]ERROR: cannot enter $path!!"
  exit 1
fi

pwd

echo "[]INFO:downloading files ..."
exit

wget -O quakes.20140101.now.m5.5+.kml "https://earthquake.usgs.gov/fdsnws/event/1/query?format=kml&starttime=2014-01-01&endtime=NOW&minmagnitude=5.5&minlatitude=20&maxlatitude=41&minlongitude=70&maxlongitude=110"
wget -O quakes.20140101.now.m5.5+.csv "https://earthquake.usgs.gov/fdsnws/event/1/query?format=csv&starttime=2014-01-01&endtime=NOW&minmagnitude=5.5&minlatitude=20&maxlatitude=41&minlongitude=70&maxlongitude=110"

wget -O quakes.20140101.now.m4.5+.kml "https://earthquake.usgs.gov/fdsnws/event/1/query?format=kml&starttime=2014-01-01&endtime=NOW&minmagnitude=4.5&minlatitude=20&maxlatitude=41&minlongitude=70&maxlongitude=110"
wget -O quakes.20140101.now.m4.5+.csv "https://earthquake.usgs.gov/fdsnws/event/1/query?format=csv&starttime=2014-01-01&endtime=NOW&minmagnitude=4.5&minlatitude=20&maxlatitude=41&minlongitude=70&maxlongitude=110"

