    OPATH, $  ;OUTPUT CME FILE
    COORDS_FILE=COORDS_FILE, $
    FILES=FILES, $
    PREFIX_NETWORK=NETEXT, $
    PREFIX_AGENCY=ORGEXT, $
    OVERWRITE=OVERWRITE, $
    CORR_TYPE=CORR_TYPE, $
    TLB=EV.TOP, $
    STATUS=STATUS
    
