file_scene_tab=scene.tab
file_scene_tab=scene.tab1

days=`ls -lS intf_all_los_ll_png/*-phasefilt_mask_ll.png | awk '{print $9}'|awk -F/ '{print $2}' | awk -F- '{print $1}'`
days=`ls -lS intf_all_los_ll_png/*-*.png | awk '{print $9}'|awk -F/ '{print $2}' | awk -F- '{print $1}'`

rm -rf intf.in.s
dos2unix ${file_scene_tab}

for day in $days; do
    echo $day
    ##
    day1=`echo $day | awk -F_ '{print $1}'`
    n1=`grep -n $day1 ${file_scene_tab} | awk -F: '{print $1}'`
    ymd1=`sed -n "${n1}p" data.in | awk '{print $1}'`
    ##
    day2=`echo $day | awk -F_ '{print $2}'`
    n2=`grep -n $day2 ${file_scene_tab} | awk -F: '{print $1}'`
    ymd2=`sed -n "${n2}p" data.in | awk '{print $1}'`
    ##
    echo $ymd1:$ymd2 $n1 $n2
    exit
    cat intf.in | grep $ymd1 | grep $ymd2 >> intf.in.s
done
