           urum
           guao
           wuhn whjf shao
           kmin KUNM
           lhas
           lhaz

OPTIONS
     NONE.

EXAMPLES
     run_cmonoc_reg --path=/solut/gmf.survey.mode/reg5
        --obstat=/solut/gmf.survey.mode/reg5/sh/STAT
        --stable=/solut/gmf.survey.mode/reg5/sh/stable.sit



