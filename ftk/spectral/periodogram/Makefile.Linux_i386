SLIB = ../../data/read/cgps_data_read.a.Linux_i386 ../../data/write/cgps_data_write.a.Linux_i386 ../../lib/cgps_lib.a.Linux_i386 ../../../../gglib/gglib.a.Linux_i386 ../../nr/nr_lib.a.Linux_i386 
