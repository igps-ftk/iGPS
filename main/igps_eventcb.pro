  ;ENSURE SIO NEU FORMAT
  IF IGPS_CHECK_TSTYPE(EV) NE 0 THEN RETURN
  ;GET SITES LIST
  IF IGPS_CHECK_LIST_SITE(EV,ALL=SITES,SELECT=SITES_SEL, INDEX=INDEX) NE 0 THEN RETURN
