all: esa_s1_manifest_overlap esa_s1_overlap_check3 is_point_in_polygon sar_s1_frame_match esa_s1_frame_intersect



# ASSIGMENTS
### G77 flags ###
#FTN = g77 
#FFLAGS = -O3 -Wuninitialized -Wunused -Wimplicit -fno-f2c -ffast-math -fno-automatic -fno-backslash -Wno-globals -fno-globals
#CC = gcc
#CFLAGS = -g -lz 
### GCC V4 gfortran flags ###
FTN = gfortran
#FFLAGS = -O2 -Wuninitialized -Wunused -Wimplicit -fno-f2c -ffast-math -fno-automatic -fno-backslash
FFLAGS = -O2 -Wuninitialized -fno-f2c -ffast-math -fno-automatic -fno-backslash -m64 -mcmodel=large
#FFLAGS = -O2 -Wuninitialized -fno-f2c -ffast-math -fno-automatic -fno-backslash -fallow-argument-mismatch  -mcmodel=medium #-m64 # -mcmodel=large
CC = gcc
CFLAGS = -g -lz 
#### For 64-bit machines:
#CFLAGS = -g --DADD64BIT

SLIB = ../../data/read/cgps_data_read.a.CYGWIN_NT-10.0_unknown ../../data/site/cgps_data_site.a.CYGWIN_NT-10.0_unknown ../../lib/cgps_lib.a.CYGWIN_NT-10.0_unknown ../../../../gglib/gglib.a.CYGWIN_NT-10.0_unknown 
X11LIB = 
II = 

esa_s1_manifest_overlap : esa_s1_manifest_overlap.f  $(SLIB) 
	$(FTN) esa_s1_manifest_overlap.f  $(SLIB) -o esa_s1_manifest_overlap.CYGWIN_NT-10.0_unknown $(FFLAGS)
	rm -f esa_s1_manifest_overlap.o

esa_s1_overlap_check3 : esa_s1_overlap_check3.f  $(SLIB) 
	$(FTN) esa_s1_overlap_check3.f  $(SLIB) -o esa_s1_overlap_check3.CYGWIN_NT-10.0_unknown $(FFLAGS)
