all: cgps_lib nblen dyr_2_yrdoyr ymd_2_dyr mean interval



# ASSIGMENTS
### G77 flags ###
#FTN = g77 
#FFLAGS = -O3 -Wuninitialized -Wunused -Wimplicit -fno-f2c -ffast-math -fno-automatic -fno-backslash -Wno-globals -fno-globals
#CC = gcc
#CFLAGS = -g -lz 
### GCC V4 gfortran flags ###
FTN = gfortran
FFLAGS = -O3 -Wuninitialized -Wunused -Wimplicit -fno-f2c -ffast-math -fno-automatic -fno-backslash
CC = gcc
CFLAGS = -g -lz 
#### For 64-bit machines:
#CFLAGS = -g --DADD64BIT

CGPS_LIB = cgps_lib.a.CYGWIN_NT-6.1-WOW_unknown
SLIB = ../../../gglib/gglib.a.CYGWIN_NT-6.1-WOW_unknown 
X11LIB = 
II = 

cgps_lib : $(CGPS_LIB)

nblen : nblen.f  $(SLIB) 
	$(FTN) nblen.f  $(SLIB) -o nblen.CYGWIN_NT-6.1-WOW_unknown $(FFLAGS)
	rm -f nblen.o

dyr_2_yrdoyr : dyr_2_yrdoyr.f  $(SLIB) 
	$(FTN) dyr_2_yrdoyr.f  $(SLIB) -o dyr_2_yrdoyr.CYGWIN_NT-6.1-WOW_unknown $(FFLAGS)
	rm -f dyr_2_yrdoyr.o

ymd_2_dyr : ymd_2_dyr.f  $(SLIB) 
	$(FTN) ymd_2_dyr.f  $(SLIB) -o ymd_2_dyr.CYGWIN_NT-6.1-WOW_unknown $(FFLAGS)
	rm -f ymd_2_dyr.o

mean : mean.f  $(SLIB) 
	$(FTN) mean.f  $(SLIB) -o mean.CYGWIN_NT-6.1-WOW_unknown $(FFLAGS)
	rm -f mean.o

interval : interval.f  $(SLIB) 
	$(FTN) interval.f  $(SLIB) -o interval.CYGWIN_NT-6.1-WOW_unknown $(FFLAGS)
	rm -f interval.o

