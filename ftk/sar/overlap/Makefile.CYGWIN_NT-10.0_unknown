	rm -f esa_s1_frame_intersect.o


.c.a:
	$(CC) -c $(CFLAGS) -I$(II) $<
	ar rv $@ *.o
	rm -f $*.o

.f.a:
	$(FTN) -c $(FFLAGS) $<
	ar rv $@ *.o
	rm -f $*.o


cleanall:
	rm -f *.a.CYGWIN_NT-10.0_unknown *.o *core esa_s1_manifest_overlap esa_s1_overlap_check3 is_point_in_polygon sar_s1_frame_match esa_s1_frame_intersect  esa_s1_manifest_overlap.CYGWIN_NT-10.0_unknown esa_s1_overlap_check3.CYGWIN_NT-10.0_unknown is_point_in_polygon.CYGWIN_NT-10.0_unknown sar_s1_frame_match.CYGWIN_NT-10.0_unknown esa_s1_frame_intersect.CYGWIN_NT-10.0_unknown
